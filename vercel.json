{
  "version": 2,
  "builds": [
    {
      "src": "myweb/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "myweb/wsgi.py"
    }
  ]
}

build_files.sh

pip install -r requirements.txt 
python3.9 manage.py collectstatic


